{
    "roots": ["<rootDir>"],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.ts?$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.ts?$": "babel-jest"
    },
    "collectCoverage": true,
    "clearMocks": true,
    "coverageDirectory": "coverage"
  }